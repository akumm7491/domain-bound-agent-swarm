{
  "name": "ai-social-media-management-platform",
  "version": "0.1.0",
  "description": "AI-powered social media management platform with domain-specific agents",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn src/server.ts",
    "docker:build": "docker build -t asmmp:latest .",
    "docker:deploy": "./scripts/deploy.sh docker",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v",
    "k8s:build": "docker build -t asmmp:latest .",
    "k8s:deploy": "./scripts/deploy.sh kubernetes",
    "k8s:delete": "kubectl delete namespace asmmp",
    "k8s:logs": "kubectl logs -f -l app=asmmp -n asmmp",
    "k8s:status": "kubectl get all -n asmmp"
  },
  "dependencies": {
    "bull": "^4.11.3",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.4.1",
    "openai": "^4.20.1",
    "prom-client": "^15.0.0",
    "telegraf": "^4.12.2",
    "twitter-api-v2": "^1.15.1",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
